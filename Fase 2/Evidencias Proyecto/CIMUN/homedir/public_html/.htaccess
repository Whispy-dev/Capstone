RewriteEngine On
RewriteCond %{HTTP_HOST} ^cimun.cl [NC]
RewriteRule ^(.*)$ https://www.cimun.cl/$1 [L,R=301]

RewriteEngine On
RewriteCond %{HTTP_HOST} mail.cimun.cl [NC]
RewriteRule ^(.*)$ https://www.cimun.cl/$1 [L,R=301]

RewriteEngine on
RewriteCond %{REQUEST_URI} !^public
RewriteRule ^(.*)$ public/$1 [L]
